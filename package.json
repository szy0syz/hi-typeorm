    "dotenv": "^8.2.0",
    "faker": "^5.1.0",
    "pg": "^8.4.1",
    "prompts": "^2.3.2",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@types/faker": "^5.1.2",
    "@types/prompts": "^2.0.9",
    "eslint": "^7.11.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
